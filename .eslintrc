{
  "extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "react/jsx-props-no-spreading": "off", // 48
    "no-shadow": "off", // 54
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}